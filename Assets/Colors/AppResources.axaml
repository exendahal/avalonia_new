<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Primary">#8b44ac</SolidColorBrush>
    <SolidColorBrush x:Key="PrimaryDark">#242C32</SolidColorBrush>
    <SolidColorBrush x:Key="Secondary">#DFD8F7</SolidColorBrush>
    <SolidColorBrush x:Key="Tertiary">#2B0B98</SolidColorBrush>
    <SolidColorBrush x:Key="WhiteColor">#FFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="ToastBodyColor">#878D96</SolidColorBrush>
    <SolidColorBrush x:Key="SuccessColor">#38B865</SolidColorBrush>
    <SolidColorBrush x:Key="FailColor">#F44336</SolidColorBrush>
    <SolidColorBrush x:Key="WarningColor">#FFC107</SolidColorBrush>
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="ToastBackground">#242C32</SolidColorBrush>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="ToastBackground">#FFFFFF</SolidColorBrush>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
