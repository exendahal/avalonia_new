<Application
    x:Class="avalonia_new.App"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    RequestedThemeVariant="Default">
    <!--  "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options.  -->
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="/Assets/Colors/AppResources.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        <FontFamily x:Key="Material-Font">avares://avalonia_new/Assets/Fonts/material-font.ttf#Material Design Icons</FontFamily>
    </Application.Resources>
    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="/Assets/Styles/AppStyles.axaml" />
    </Application.Styles>
</Application>
